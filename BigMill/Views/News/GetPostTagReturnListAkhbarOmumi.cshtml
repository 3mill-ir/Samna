@{
    ViewBag.Title = "GetTagReturnListAkhbarOmumi";
}
@model PagedList.IPagedList<BigMill.Models.PostModel>
@using PagedList.Mvc;
@section CSS_Requested{

    <!-- Google Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Marcellus:400,400italic,700,700italic%7CMarcellus:400,400italic,700,700italic%7CMarcellus:400,400italic,700,700italic%7CMarcellus:400,400italic,700,700italic%7CMarcellus:400,400italic,700,700italic%7CMarcellus:400,400italic,700,700italic%7CPT+Sans:400,400italic,700,700italic%7CPT+Sans:400,400italic,700,700italic%7C' rel='stylesheet' type='text/css'>

    <!-- Stylesheets -->
    <link rel='stylesheet' id='stylesheet-css' href='~/Content/UserContent/css/style.css?ver=1.1' type='text/css' media='all' />
    <link rel='stylesheet' id='icons-css' href='~/Content/UserContent/css/icons.css?ver=1.0' type='text/css' media='all' />
    <link rel='stylesheet' id='animate-css' href='~/Content/UserContent/css/animate.css?ver=1.0' type='text/css' media='all' />
    <link rel='stylesheet' id='shortcodes_front-css' href='~/Content/UserContent/css/shortcodes.css?ver=4.3.2' type='text/css' media='all' />
    <link rel='stylesheet' id='jplayer-css' href='~/Content/UserContent/css/jplayer.css?ver=1.0' type='text/css' media='all' />


    <!-- Custom CSS -->
    <link rel='stylesheet' id='custom-css' href='~/Content/UserContent/css/custom-styles.css' type='text/css' media='all' />
    <!-- Responsive CSS -->
    <!-- RTL CSS -->
    <link rel='stylesheet' id='rtl-css' href='~/Content/UserContent/css/rtl.css' type='text/css' media='all' />
}
@section js_Requested{

    <script type='text/javascript' src='~/Content/UserContent/js/jquery/jquery.js?ver=1.11.0'></script>
    <script type='text/javascript' src='~/Content/UserContent/js/themetor.js?ver=1.0'></script>
    <script type='text/javascript' src='~/Content/UserContent/js/tt_composer_front.js?ver=4.3.2'></script>
    <script type='text/javascript' src='~/Content/UserContent/js/jquery.prettyPhoto.js?ver=3.1.6'></script>
    <script type='text/javascript' src='~/Content/UserContent/js/jquery.flexslider-min.js?ver=2.1'></script>
    <script type='text/javascript' src='~/Content/UserContent/js/jquery.jplayer.min.js?ver=2.4.0'></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js"></script>
    <script type='text/javascript' src='~/Content/UserContent/js/custom.js?ver=1.0'></script>

    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            try { js_audioPlayer('jplayer_1345433225', 'http://theme20.com/demo/remix/Corporate Theme/5.mp3'); } catch (e) { }
        });
    </script>

}

@section BreadCrumb{
    <div class="breadcrumb-place ">
        <div class="row clearfix">
            <h1 class="page-title">Blog Both Sidebars</h1>
        </div><!-- row -->
    </div>
}



<div class="page-content">
    <div class="row clearfix">

        <!-- start sidebar -->
        <div class="grid_3 alpha sidebar">

            <div id="official_ads-4" class="widget widget_official_ads">
                <h3 class="col-title">Sponsors</h3>
                <span class="liner"></span>
                <div class="ads clearfix">
                    <a href="http://www.themetor.com" target="_blank"><img src="~/Content/UserContent/images/ads125.png" /></a>
                    <a href="http://www.themetor.com" target="_blank"><img src="~/Content/UserContent/images/ads125.png" /></a>
                </div>
            </div>

            <div id="search-2" class="widget widget_search">
                <form role="search" method="get" id="searchform" action="#">
                    <input type="text" value="" name="s" id="s" placeholder="Search..." />
                    <input type="submit" id="searchsubmit" value="&#61442;" />
                </form>
            </div>

            <div id="official_combo_tabs-3" class="widget widget_official_combo_tabs">
                <div class="tabswrap">
                    <ul class="tabs">
                        <li><a class="active" href="#panel-tt1">@Resource.Resource.User_PopularPosts</a></li>
                        <li><a href="#panel-tt2">@Resource.Resource.User_RecentPosts</a></li>
                        <li><a href="#panel-tt4" style="width: 48px;">@Resource.Resource.User_Tags</a></li>
                    </ul>
                    <ul class="tabs-content">

                        <li class="active" id="panel-tt1">
                            <ul class="mostpopular">
                                @Html.Action("PopularPosts", "Utility")
                            </ul>
                        </li>

                        <li id="panel-tt2">
                            <ul class="recentposts">
                                @Html.Action("RecentPosts", "Utility")
                            </ul>
                        </li>

                        <li id="panel-tt4">
                            @Html.Action("SubCategoryTags", "Utility", new { SubCategoryId = System.Configuration.ConfigurationManager.AppSettings["AkhbarOmumiId"] })
                        </li>
                    </ul>

                </div>
                <!-- tabswrap -->

            </div><!-- tabs -->

            <div class="widget widget_categories">
                <h3 class="col-title">Categories</h3><span class="liner"></span>
                <ul>
                    <li class="cat-item"><a href="single-image.html" title="View all posts filed under Fashion">Fashion</a></li>
                    <li class="cat-item"><a href="single-qoute.html" title="View all posts filed under News">News</a></li>
                    <li class="cat-item"><a href="single-voice.html" title="View all posts filed under Uncategorized">Uncategorized</a></li>
                </ul>
            </div>

            <div class="widget widget_official_facebook">
                <h3 class="col-title">Like Us</h3><span class="liner"></span>
                <iframe src="http://www.facebook.com/plugins/likebox.php?href=http%3A%2F%2Fwww.facebook.com%2Fenvato&amp;width=340&amp;colorscheme=light&amp;show_faces=true&amp;stream=false&amp;header=true&amp;height=402&amp;border_color=%23FFF" scrolling="no" frameborder="0" style="overflow:hidden; width:340px; height: 402px;" allowtransparency="true"></iframe>
            </div>

            <div class="widget widget_official_subscription">
                <h3 class="col-title">Join Us</h3>
                <span class="liner"></span>

                <form method="post" id="subscriptionform" action="#" target="popupwindow">
                    <input type="email" name="Email" class="inputer" placeholder="Type Your Email" required>
                    <input type="submit" id="subscribe-button" value="&#61694;" />
                </form>

            </div>

            <div class="widget widget_recent_comments">
                <h3 class="col-title">Recent Comments</h3><span class="liner"></span>
                <ul id="recentcomments">
                    <li class="recentcomments">admin on <a href="single-image.html#comment-2">Austin viral accusamus put a bird on it</a></li>
                    <li class="recentcomments">Maria on <a href="single-image.html/#comment-36">Woo Ninja</a></li>
                    <li class="recentcomments">Maria on <a href="single-image.html/#comment-35">Premium Quality</a></li>
                    <li class="recentcomments">Maria on <a href="single-image.html/#comment-29">Woo Logo</a></li>
                    <li class="recentcomments">Maria on <a href="http://demo.themetor.com/official/demo22/product/ninja-silhouette-2/#comment-26">Ninja Silhouette</a></li>
                </ul>
            </div>

            <div id="archives-2" class="widget widget_archive">
                <h3 class="col-title">Archives</h3><span class="liner"></span>
                <ul>
                    <li><a href='#'>July 2015</a></li>
                </ul>
            </div>

        </div><!-- grid 3 Sidebar -->

        <div class="grid_6 posts both-sidebars">
            <!-- post -->
            @foreach (var item in Model)
            {
                <div class="post type-post status-publish format-standard has-post-thumbnail hentry category-news clearfix">
                    <a href="single-image.html" class="thumb-big">
                        <img src="~/Content/UplodedImages/PostImages/@item.Content_Two" alt="Image ALt" />
                    </a>
                    <div class="content">
                        <div class="meta-box">
                            <img alt='Image Alt' src='~/Content/UserContent/images/80x80.png' class='image-author  photo' />
                            <h3><a href="single-image.html">@item.Content_One</a></h3>
                            <div class="meta-more">
                                <span><i class="icon-user"></i> <a href="#" title="Posts by admin" rel="author">admin</a></span>
                                <span><i class="icon-time"></i><a href="#">@item.CreateDateUtcJalali</a></span>
                                <span><i class="icon-comments"></i> <a href="single-image.html#respond" title="Comment on Mumblecore sapiente pork belly">@item.NumberOfComments</a></span>
                                <span><i class="icon-thumbs-up"></i> <a href="#" title="Comment on Mumblecore sapiente pork belly">@item.NumberOfLikes</a></span>
                                <span><i class="icon-thumbs-down"></i> <a href="#" title="Comment on Mumblecore sapiente pork belly">@item.NumberOfDisLikes</a></span>
                            </div>
                            <!-- meta date -->
                        </div>
                        <p>@item.Content_Three</p>
                        <a href="@Url.Action("AkhbarOmumi_Detail", "News", new { PostId = item.ID })" class="tbutton tbutton1 small"><span>@Resource.Resource.User_MoreDetails</span></a>
                    </div>
                </div>
            }

            <!--end post -->
            <!-- PAGINATION -->
            <p class="hide"></p>
            <!-- END PAGINATION -->

            <div style="text-align:center;direction:ltr!important" class="pagination-tt clearfix">
                صفحه @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) از @Model.PageCount
                @Html.PagedListPager(Model, page => Url.Action("GetPostTagReturnListAkhbarOmumi","News", new { page,TagId=1}))
            </div>

        </div><!-- grid 6 posts -->
        <!-- start sidebar -->
        <div class="grid_3 sidebar omega">

            <div class="widget widget_official_ads">
                <h3 class="col-title">Ads</h3><span class="liner"></span>
                <div class="ads clearfix">
                    <a href="#" target="_blank"><img src="~/Content/UserContent/images/200x90.jpg" /></a>
                    <a href="#" target="_blank"><img src="~/Content/UserContent/images/200x90.jpg" /></a>
                </div>
            </div>

            <div class="widget widget_official_subscription">
                <h3 class="col-title">Join Our Newsletter</h3><span class="liner"></span>
                <form method="post" id="subscriptionform" action="#" target="popupwindow">
                    <input type="email" name="Email" class="inputer" placeholder="Type Your Email" required>
                    <input type="submit" id="subscribe-button" value="&#61694;" />
                </form>
            </div>

            <div class="widget widget_categories">
                <h3 class="col-title">Categories</h3>
                <span class="liner"></span>
                <ul>
                    <li class="cat-item"><a href="single-image.html" title="View all posts filed under Fashion">Fashion</a> (2)</li>
                    <li class="cat-item"><a href="single-qoute.html" title="View all posts filed under News">News</a> (3)</li>
                    <li class="cat-item"><a href="#" title="View all posts filed under Uncategorized">Uncategorized</a> (2)</li>
                </ul>
            </div>

            <div id="recent-posts-4" class="widget widget_recent_entries">
                @Html.Action("RssFeed", "Utility")
            </div>

        </div><!-- grid 3 Sidebar -->

    </div><!-- row -->

</div>