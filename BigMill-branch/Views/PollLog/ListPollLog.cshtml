@model IEnumerable<BigMill.Models.PollLogModel>

<p>
    @Html.ActionLink("Create New", "AddPollLog")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Device)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateOnUTC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.F_PollAnswer_Id)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Device)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateOnUTC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.F_PollAnswer_Id)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditPollLog", new { PollLogId = item.ID }) |
            @Html.ActionLink("ChangeStatus", "ChangeStatusPollLog", new { PollLogId = item.ID }) |
            @Html.ActionLink("Delete", "DeletePollLog", new { PollLogId = item.ID })
        </td>
    </tr>
}

</table>
