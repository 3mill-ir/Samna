@model IEnumerable<BigMill.Models.UserInformationModel>

<p>
    @Html.ActionLink("Create New", "AddUserInformation")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatedOnUTC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedOnUTC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.F_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedOnUTC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedOnUTC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.F_ID)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditUserInformation", new { UserInformationId = item.ID }) |
            @Html.ActionLink("ChangeStatus", "ChangeStatusUserInformation", new { UserInformationId = item.ID }) |
            @Html.ActionLink("Delete", "DeleteUserInformation", new { UserInformationId = item.ID })
        </td>
    </tr>
}

</table>
