@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    Layout = null;
}



<!DOCTYPE html>
<!-- Template Name: 3mill - Responsive Admin Template build with Twitter Bootstrap 3.x | Author: 3mill -->
<!--[if IE 8]><html class="ie8" lang="en"><![endif]-->
<!--[if IE 9]><html class="ie9" lang="en"><![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<!-- start: HEAD -->
<!-- start: HEAD -->
<head>
    <title>@Resource.Resource.View_Tittle</title>
    <!-- start: META -->
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content="IE=edge,IE=9,IE=8,chrome=1" /><![endif]-->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- end: META -->
    <!-- start: GOOGLE FONTS -->
    <link href="http://fonts.googleapis.com/css?family=Lato:300,400,400italic,600,700|Raleway:300,400,500,600,700|Crete+Round:400italic" rel="stylesheet" type="text/css" />
    <!-- end: GOOGLE FONTS -->
    <!-- start: MAIN CSS -->
    <link rel="stylesheet" href="~/Content/AdminContent/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/AdminContent/vendor/bootstrap/css/bootstrap-rtl.min.css">
    <link rel="stylesheet" href="~/Content/AdminContent/vendor/fontawesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/AdminContent/vendor/themify-icons/themify-icons.min.css">
    <link href="~/Content/AdminContent/vendor/animate.css/animate.min.css" rel="stylesheet" media="screen">
    <link href="~/Content/AdminContent/vendor/perfect-scrollbar/perfect-scrollbar.min.css" rel="stylesheet" media="screen">
    <link href="~/Content/AdminContent/vendor/switchery/switchery.min.css" rel="stylesheet" media="screen">
    <!-- end: MAIN CSS -->
    <!-- start: 3mill CSS -->
    <link rel="stylesheet" href="~/Content/AdminContent/assets/css/styles.css">
    <link rel="stylesheet" href="~/Content/AdminContent/assets/css/plugins.css">
    <link rel="stylesheet" href="~/Content/AdminContent/assets/css/themes/theme-1.css" id="skin_color" />
    <link rel="stylesheet" href="~/Content/AdminContent/assets/css/rtl.css" />
    <!-- end: 3mill CSS -->
    <!-- start: CSS REQUIRED FOR THIS PAGE ONLY -->
    <link href="~/Content/AdminContent/vendor/ladda-bootstrap/ladda-themeless.min.css" rel="stylesheet" media="screen">
    <!-- end: CSS REQUIRED FOR THIS PAGE ONLY -->
</head>
<!-- end: HEAD -->
<!-- start: BODY -->
<body class="login">
    <!-- start: REGISTRATION -->
    <div class="row">
        <div class="main-login col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2 col-md-4 col-md-offset-4">
            <div class="logo margin-top-30">
                <img src="~/Content/AdminContent/assets/images/logo.png" alt="3mill" />
            </div>
            <!-- start: REGISTER BOX -->
                <div class="box-register">

                    <fieldset>
                        <legend>
                            نام نقش
                        </legend>
                        <div class="table-responsive">
                            <table class="table table-hover table-striped " id="sample-table-1">
                                <thead>
                                    <tr>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@Html.DisplayFor(modelItem => item.Name)</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>

                        <div class="form-actions pull-left">
                            <a type="button" class="btn btn-primary btn-wide btn-scroll btn-scroll-top  ti-plus" href="@Url.Action("Create")">
                                <span>تعریف نقش جدید</span>
                            </a>

                        </div>

                    </fieldset>

                    <!-- start: COPYRIGHT -->
                    <div class="copyright">
                        &copy; <span class="current-year"></span><span class="text-bold text-uppercase"> @Resource.Resource.CMS_Name</span>
                    </div>
                    <!-- end: COPYRIGHT -->
                </div>
         
            <!-- end: REGISTER BOX -->
        </div>
    </div>
    <!-- end: REGISTRATION -->
    <!-- start: MAIN JAVASCRIPTS -->
    <script src="~/Content/AdminContent/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/AdminContent/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/AdminContent/vendor/modernizr/modernizr.js"></script>
    <script src="~/Content/AdminContent/vendor/jquery-cookie/jquery.cookie.js"></script>
    <script src="~/Content/AdminContent/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Content/AdminContent/vendor/switchery/switchery.min.js"></script>
    <!-- end: MAIN JAVASCRIPTS -->
    <!-- start: JAVASCRIPTS REQUIRED FOR THIS PAGE ONLY -->
    <script src="~/Content/AdminContent/vendor/jquery-validation/jquery.validate.min.js"></script>
    <script src="~/Content/AdminContent/vendor/ladda-bootstrap/spin.min.js"></script>
    <script src="~/Content/AdminContent/vendor/ladda-bootstrap/ladda.min.js"></script>
    <!-- end: JAVASCRIPTS REQUIRED FOR THIS PAGE ONLY -->
    <!-- start: 3mill JAVASCRIPTS -->
    <script src="~/Content/AdminContent/assets/js/main.js"></script>
    <!-- start: JavaScript Event Handlers for this page -->
    <script src="~/Content/AdminContent/assets/js/ui-buttons.js"></script>
    <script>
        jQuery(document).ready(function () {
            Main.init();
            UIButtons.init();
        });
    </script>
    <!-- end: JavaScript Event Handlers for this page -->
    <!-- end: 3mill JAVASCRIPTS -->
</body>
<!-- end: BODY -->
</html>
